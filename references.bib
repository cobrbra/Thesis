	editor = {Huang, De-Shuang and Bevilacqua, Vitoantonio and Premaratne, Prashan and Gupta, Phalguni},
	year = {2017},
	keywords = {Activation function, Convolutional layer, Convolutional neural network, Image recognition, Pooling layer},
